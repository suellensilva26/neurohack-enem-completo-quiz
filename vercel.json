{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/vendas/(.*)",
      "dest": "/vendas/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
